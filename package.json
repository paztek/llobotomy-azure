{
    "name": "llobotomy-azure",
    "version": "0.0.10",
    "description": "Thin wrapper around the Azure OpenAI Chat Completions API + Tools Calling to emulate the missing Assistants API",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "types/index.d.ts",
    "files": [
        "dist/",
        "types/"
    ],
    "type": "commonjs",
    "exports": {
        ".": [
            {
                "import": "./dist/index.mjs",
                "require": "./dist/index.cjs",
                "default": "./dist/index.js"
            },
            "./dist/index.js"
        ]
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@types/eslint": "^8.44.3",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.10.0",
        "@types/prettier": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "chalk": "^4.1.2",
        "dotenv": "^16.3.1",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.7.0",
        "jest-mock-extended": "^3.0.5",
        "prettier": "^3.0.3",
        "rollup": "^3.29.4",
        "rollup-plugin-typescript2": "^0.36.0",
        "ts-jest": "^29.1.1",
        "ts-jest-resolver": "^2.0.1",
        "ts-node-dev": "^2.0.0",
        "typedoc": "^0.25.1",
        "typedoc-plugin-markdown": "^3.16.0",
        "typescript": "^5.2.2"
    },
    "scripts": {
        "doc": "typedoc src/index.ts",
        "test": "jest --passWithNoTests",
        "lint": "eslint \"*/**/*.{ts,js,json}\"",
        "lint:fix": "eslint \"*/**/*.{ts,js,json}\" --fix",
        "build": "rollup --config ./rollup.config.mjs",
        "build:watch": "rollup --config ./rollup.config.mjs --watch",
        "prepublishOnly": "npm run doc && npm run lint && npm run test && npm run build",
        "examples:insurance": "ts-node-dev examples/insurance/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/paztek/llobotomy-azure.git"
    },
    "keywords": [],
    "author": {
        "name": "Matthieu Balmes"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paztek/llobotomy-azure/issues"
    },
    "homepage": "https://github.com/paztek/llobotomy-azure#readme",
    "dependencies": {
        "@azure/openai": "^1.0.0-beta.12"
    }
}
